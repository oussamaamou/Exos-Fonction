#include <stdio.h>

int Produit(int x, int y){
    return(x*y);
}
int main(){
    int a, b, P;
    printf("Veuillez entrer la valeur du premier nombre:");
    scanf("%d",&a);
    printf("Veuillez entrer la valeur du deuxieme nombre:");
    scanf("%d",&b);
    P = Produit(a,b);
    printf("Le produit des deux nombres est : %d",P);
    

    return 0;
}
